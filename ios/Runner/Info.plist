<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Glamify</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>Glamify</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>UIStatusBarHidden</key>
		<false/>
		<!-- Camera and Photo Library Permissions -->
		<key>NSCameraUsageDescription</key>
		<string>يحتاج التطبيق للكاميرا لالتقاط صور المنتجات والإعلانات</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>يحتاج التطبيق للوصول للمعرض لاختيار الصور</string>
		<key>NSPhotoLibraryAddUsageDescription</key>
		<string>يحتاج التطبيق لحفظ الصور في المعرض</string>
		<!-- Network and Internet Permissions -->
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<false/>
			<key>NSExceptionDomains</key>
			<dict>
				<key>upload.imagekit.io</key>
				<dict>
					<key>NSExceptionAllowsInsecureHTTPLoads</key>
					<false/>
					<key>NSExceptionMinimumTLSVersion</key>
					<string>TLSv1.2</string>
					<key>NSExceptionRequiresForwardSecrecy</key>
					<false/>
				</dict>
				<key>firebase.googleapis.com</key>
				<dict>
					<key>NSExceptionAllowsInsecureHTTPLoads</key>
					<false/>
					<key>NSExceptionRequiresForwardSecrecy</key>
					<false/>
				</dict>
				<key>firestore.googleapis.com</key>
				<dict>
					<key>NSExceptionAllowsInsecureHTTPLoads</key>
					<false/>
					<key>NSExceptionRequiresForwardSecrecy</key>
					<false/>
				</dict>
			</dict>
		</dict>
		<!-- File Access Permission -->
		<key>NSDocumentsFolderUsageDescription</key>
		<string>يحتاج التطبيق للوصول للملفات لحفظ الفواتير</string>
		<!-- Background Modes for Notifications -->
		<key>UIBackgroundModes</key>
		<array>
			<string>remote-notification</string>
		</array>
		<!-- Notification Settings -->
		<key>NSUserNotificationAlertStyle</key>
		<string>alert</string>
		<!-- Status Bar Configuration -->
		<key>UIStatusBarHidden</key>
		<false/>
		<key>UIViewControllerBasedStatusBarAppearance</key>
		<true/>
		<key>UIStatusBarStyle</key>
		<string>UIStatusBarStyleLightContent</string>
		<!-- Google Sign-In URL Scheme -->
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>com.googleusercontent.apps.677899943891-3brjn65v46vplgat2u5f9g19f1shhsf4</string>
				</array>
			</dict>
		</array>
		<!-- External Apps Query Schemes -->
		<key>LSApplicationQueriesSchemes</key>
		<array>
			<string>whatsapp</string>
			<string>telprompt</string>
			<string>tel</string>
			<string>mailto</string>
			<string>https</string>
			<string>http</string>
		</array>
		<!-- Notifications Permission -->
		<key>NSUserNotificationsUsageDescription</key>
		<string>يحتاج التطبيق للإشعارات لإعلامك بحالة طلباتك والعروض الجديدة</string>
	</dict>
</plist>
